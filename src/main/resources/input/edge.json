{
  "$schema" : "http://json-schema.org/draft-07/schema#",
  "description" : "format of edge in the graph",
  "type" : "object",
  "properties" : {
    "name" : {
      "type" : "string"
    },
    "from" : {
      "type" : "string"
    },
    "to" : {
      "type" : "string"
    },
    "weight" : {
      "type" : "number",
      "default" : 1
    }
  },
  "required" : [
    "name",
    "from",
    "to"
  ]
}